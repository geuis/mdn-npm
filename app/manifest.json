{
  "name": "mdn-npm",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Search both MDN and NPM from the url omnibox.",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts.babel/chromereload.js",
      "scripts.babel/background.js"
    ]
  },
  "permissions": [
    "background",
    "https://developer.mozilla.org/*",
    "https://*.npmjs.com/*"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "MDN and NPM Search",
    "default_popup": "popup.html"
  },
  "omnibox": {
    "keyword": "mmm"
  }
}
